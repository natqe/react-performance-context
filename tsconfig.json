{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "sourceMap": true,
    "outDir": "./build",
    "baseUrl": "./src",
    "sourceRoot": "./src",
    "declaration": true,
    "removeComments": false,
    "noLib": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
  },
  "include": [
    "src"
  ]
}
